{
  "name": "speicher210/kontakt.io-api",
  "type": "library",
  "description": "PHP wrapper for kontakt.io API",
  "keywords": [
    "speicher210",
    "kontakt.io"
  ],
  "homepage": "https://github.com/speicher210/kontakt.io-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Dragos Protung",
      "email": "dragos@protung.ro",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~5.6|~7.0",
    "guzzlehttp/guzzle": "~6.2",
    "jms/serializer": "~1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "scrutinizer/ocular": "~1.3",
    "squizlabs/php_codesniffer": "~2.7"
  },
  "autoload": {
    "psr-4": {
